{
    "inbounds": [
      {
        "port": 29609,
        "protocol": "vmess",
        "settings": {
          "clients": [
            {
              "id": "$UUID",
              "alterId": 0
            }
          ],
          "disableInsecureEncryption": true
        },
        "streamSettings": {
          "network": "ws",
          "wsSettings": {
            "path": "$WS_PATH"
          }
        }
      }
    ],
    "outbounds": [
      {
        "protocol": "freedom"
      }
    ]
  }
